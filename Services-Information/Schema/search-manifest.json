{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Schema for the Search Manifest input parameters",
	"$ref": "#/definitions/SearchManifest",
	"definitions": {
		"SearchManifest": {
			"description": "Input parameters for the Search Manifest service.",
			"type": "object",
			"properties": {
				"stateCode": {
					"description": "State Code",
					"type": "string"
				},
				"status": {
					"description": "Manifest Status",
					"type": "string",
					"enum": [
						"Scheduled",
						"InTransit",
						"Received",
						"ReadyForSignature",
						"Signed"
					]
				},
				"siteType": {
					"description": "Site type. If Site type is not passed then Manifest for both Generator and TSDF will be obtained",
					"type": "string",
					"enum": [
						"Generator",
						"Tsdf",
						"Transporter"
					]
				},
				"dateType": {
					"description": "Type of the Date/time of the search",
					"type": "string",
					"enum": [
						"CertifiedDate",
						"ReceivedDate",
						"ShippedDate",
						"UpdatedDate"
					]
				},
				"startDate": {
					"type": "string",
					"format": "date-time"
				},
				"endDate": {
					"type": "string",
					"format": "date-time"
				}
			},
			"required": [
				"stateCode"
			],
			"additionalProperties": false
		}
	}
}